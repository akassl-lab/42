#include <stdio.h>


int	ft_iterative_factorial(int nb);
int factorial(int n);

int	main(void)
{
	printf("Our Function %d\n", ft_iterative_factorial(5));
	return (0);
}


int	ft_iterative_factorial(int nb)
{
	if (nb < 0)
		return (0);

	int		i[2];

	i[0] = 1;
	i[1] = 1;
	while (i[0] <= nb)
	{
		i[1] *= i[0];
		i[0]++;	
	}
		
	return (i[1]);
}
